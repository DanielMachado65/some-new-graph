# @name login
POST {{host}}/auth/login
Content-Type: application/json

{
  "username" : "{{username}}",
  "password": "{{password}}"
}

###

@token = {{login.response.body.$.body.token}}
@supper_token = {{login.response.body.$.body.supper_token}}

###

GET {{host}}/api/protected/plan
Content-Type: application/json
Authorization: {{token}}
Supper: {{supper_token}}

###